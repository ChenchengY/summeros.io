{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.20.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "lint": "lint-staged",
    "commitizen": "commitizen init cz-conventional-changelog --save-dev --save-exact",
    "commit": "cz"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,mjs}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-fonts": "^0.4.0",
    "web-vitals": "^2.1.0"
  },
  "resolutions": {
    "coa": "2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "volta": {
    "node": "16.15.1"
  }
}
